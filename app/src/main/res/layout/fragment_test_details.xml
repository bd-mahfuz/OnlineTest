<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.test.onlinetest.fragments.TestDetailsFragment">


    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="392dp"
        android:layout_height="98dp"
        android:layout_marginBottom="140dp"
        android:background="@color/colorPrimaryDark"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/relativeLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"></LinearLayout>

    <TextView
        android:id="@+id/dTitleTv"
        android:layout_width="267dp"
        android:layout_height="69dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/border"
        android:elevation="10dp"
        android:gravity="center"
        android:text="Software Engineering"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="20sp"
        android:clickable="true"
        android:linksClickable="true"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        app:layout_constraintVertical_bias="0.467" />

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="72dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="81dp"
        android:elevation="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dTitleTv">


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="33dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/circle_border"
            android:padding="10dp"
            android:src="@drawable/calendar" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/imageView"
            android:layout_marginTop="10dp"
            android:background="@color/colorPrimary" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/dStartDateTv"
            android:layout_marginStart="27dp"
            android:layout_toEndOf="@+id/imageView"
            android:text="Test start on:"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/dStartDateTv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView5"
            android:layout_alignStart="@+id/textView7"
            android:layout_marginTop="5dp"
            android:text="Date: 23/4/2018"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="15sp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignStart="@+id/imageView"
            android:layout_below="@+id/textView5"
            android:layout_marginTop="15dp"
            android:background="@drawable/circle_border"
            android:elevation="5dp"
            android:padding="10dp"
            android:src="@drawable/check_mark" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/dStartDateTv"
            android:layout_below="@+id/textView5"
            android:layout_marginTop="12dp"
            android:text="Total Marks of the Test:"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/dTotalMarksTv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView2"
            android:layout_alignStart="@+id/textView9"
            android:text="Marks: 40"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/dTotalMarksTv"
            android:layout_marginTop="10dp"
            android:background="@color/colorPrimary"
            android:text="TextView" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignStart="@+id/imageView2"
            android:layout_below="@+id/textView6"
            android:layout_marginTop="15dp"
            android:background="@drawable/circle_border"
            android:elevation="5dp"
            android:padding="10dp"
            android:src="@drawable/hourglass" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/dTotalMarksTv"
            android:layout_below="@+id/textView6"
            android:layout_marginTop="12dp"
            android:text="Total Time of the Test:"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/dEstTimeTv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView3"
            android:layout_alignStart="@+id/textView10"
            android:text="Time: 2 hours 30 minuit"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/dEstTimeTv"
            android:layout_marginTop="10dp"
            android:background="@color/colorPrimary"
            android:text="TextView" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignStart="@+id/imageView3"
            android:layout_below="@+id/textView8"
            android:layout_marginTop="15dp"
            android:background="@drawable/circle_border"
            android:elevation="5dp"
            android:padding="10dp"
            android:src="@drawable/parking" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/dEstTimeTv"
            android:layout_below="@+id/textView8"
            android:layout_marginTop="12dp"
            android:text="Fee amount the Test:"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/dFeeTv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView4"
            android:layout_alignStart="@+id/textView11"
            android:text="Fee: 40$"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="15sp" />

        <!--<TextView
            android:id="@+id/textView14"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/dFeeTv"
            android:layout_marginTop="10dp"
            android:background="@color/colorPrimary"
            android:text="TextView" />-->

    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
